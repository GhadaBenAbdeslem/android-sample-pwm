<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.digi.android.sample.pwm"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-permission android:name="com.digi.android.permission.PWM"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/digi_icon"
        android:label="@string/app_name">
        <uses-library
            android:name="com.digi.android"
            android:required="true" />

        <activity
            android:name="com.digi.android.sample.pwm.PWMSampleActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>